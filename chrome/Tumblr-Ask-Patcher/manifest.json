{
   "name": "Tumblr Ask Patcher",
   "version": "1.1",
   "manifest_version": 2,
   "description": "Allows line breaks and links in asks.",
   "update_url": "https://github.com/jesstech/Tumblr-Ask-Patcher/raw/master/chrome/update-manifest.xml",
   "content_scripts": [
    {
      "matches": ["*://*.tumblr.com/ask_form/*"],
      "all_frames": true,
      "js": ["askPatcher.js"]
    }
   ]
}